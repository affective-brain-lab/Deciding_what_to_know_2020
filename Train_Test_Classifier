library("flexclust")

data <- na.omit(Data)

dat <- data[1:3]
ind<- data[4:6]
dat[["train"]] <-TRUE
ind[["train"]] <-FALSE
cl1 <- kcca(dat[dat[["train"]]==TRUE, 1:3], k= 6, family=kccaFamily("kmeans"))
cl1

pred_train <- predict(cl1)
pred_test<- predict(cl1, newdata=ind[ind[["train"]]==FALSE, 1:3])
sum(pred_test==pred_train)/123
